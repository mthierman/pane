{
    "name": "pane",
    "version": "0.0.0",
    "type": "module",
    "private": true,
    "exports": {
        ".": {
            "import": "./pane/src-typescript/index.ts",
            "types": "./pane/src-typescript/index.d.ts"
        }
    },
    "scripts": {
        "check": "tsc -b",
        "webview": "vite examples/webview/gui",
        "webview:host": "vite examples/webview/gui --host",
        "webview:build": "tsc -b && vite build examples/webview/gui",
        "webview:watch": "vite build examples/webview/gui --watch",
        "webview:preview": "tsc -b && vite build examples/webview/gui && vite preview examples/webview/gui"
    },
    "devDependencies": {
        "@types/node": "24.3.0",
        "prettier": "^3.6.2",
        "typescript": "^5.9.2",
        "vite": "^7.1.2"
    }
}
