{
    "name": "pane",
    "version": "0.0.0",
    "type": "module",
    "private": true,
    "exports": {
        ".": "./pane/typescript/index.ts",
        "./webview": "./pane/typescript/webview.ts"
    },
    "scripts": {
        "check": "tsc",
        "example:dev": "vite examples/webview/gui",
        "example:host": "vite examples/webview/gui --host",
        "example:build": "tsc && vite build examples/webview/gui",
        "example:watch": "tsc && vite build examples/webview/gui --watch",
        "example:preview": "tsc -b && vite build examples/webview/gui && vite preview examples/webview/gui"
    },
    "devDependencies": {
        "@types/node": "24.3.0",
        "prettier": "^3.6.2",
        "typescript": "^5.9.2",
        "vite": "^7.1.3",
        "vite-plugin-singlefile": "^2.3.0"
    }
}
